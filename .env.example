# Telegram API (service account for public channels)
MASTER_API_ID=
MASTER_API_HASH=

# Postgres / Supabase
POSTGRES_DB=postgres
POSTGRES_PASSWORD=postgres

# Database URLs (containers/host)
DATABASE_URL=postgresql://postgres:${POSTGRES_PASSWORD}@supabase-db:5432/${POSTGRES_DB}
DATABASE_URL_LOCAL=postgresql://postgres:${POSTGRES_PASSWORD}@localhost:5432/${POSTGRES_DB}

# Supabase keys (required by Studio/Kong)
ANON_KEY=REPLACE_ME
SERVICE_KEY=REPLACE_ME

# JWT (single secret used by GoTrue/PostgREST/your services)
JWT_SECRET=REPLACE_ME_LONG_RANDOM
JWT_ALGORITHM=HS256

# Redis / Qdrant (containers)
REDIS_URL=redis://redis:6379
QDRANT_URL=http://qdrant:6333
# QDRANT_URL_LOCAL=http://localhost:6333

# App
ENVIRONMENT=production
LOG_LEVEL=INFO

# Domains
DOMAIN=produman.studio
SUPABASE_HOST=supabase.produman.studio

# Grafana
GF_SECURITY_ADMIN_PASSWORD=admin

# Embeddings (Worker)
EMBEDDING_MODEL=hash-based
EMBEDDING_DIMENSION=128