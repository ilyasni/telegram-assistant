{
  "dashboard": {
    "id": null,
    "title": "QR Auth Funnel - Telegram Assistant",
    "tags": ["telegram", "auth", "qr", "funnel"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "QR Auth Funnel",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(auth_qr_start_total[5m]))",
            "legendFormat": "QR Started"
          },
          {
            "expr": "sum(rate(auth_qr_published_total[5m]))",
            "legendFormat": "QR Published"
          },
          {
            "expr": "sum(rate(auth_qr_success_total[5m]))",
            "legendFormat": "QR Authorized"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "QR Auth Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(auth_qr_success_total[5m])) / sum(rate(auth_qr_start_total[5m])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "green",
                  "value": 50
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "title": "QR Auth Errors",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(auth_qr_fail_total[5m]))",
            "legendFormat": "Errors/min"
