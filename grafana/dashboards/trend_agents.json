{
  "uid": "trend-agents-monitoring",
  "title": "Trend Agents Monitoring",
  "tags": [
    "trends",
    "agents",
    "quality"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Trend Editor Agent - Requests Total",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(trend_editor_requests_total[5m])) by (outcome) or vector(0)",
          "legendFormat": "{{outcome}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2
        }
      }
    },
    {
      "id": 2,
      "title": "Trend Editor Agent - Quality Score (95th percentile)",
      "type": "graph",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(trend_editor_quality_score_bucket[5m])) by (le)) or vector(0)",
          "legendFormat": "95th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(trend_editor_quality_score_bucket[5m])) by (le)) or vector(0)",
          "legendFormat": "50th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Quality Score"
        },
        {
          "format": "short"
        }
      ]
    },
