{
  "dashboard": {
    "title": "Album Pipeline Monitoring",
    "tags": [
      "albums",
      "pipeline",
      "monitoring"
    ],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Albums Parsed Rate",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "rate(albums_parsed_total[5m])",
            "refId": "A",
            "legendFormat": "{{status}}",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Albums Assembled Rate",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "rate(albums_assembled_total[5m])",
            "refId": "A",
            "legendFormat": "{{status}}",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Album Assembly Lag (p95)",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(album_assembly_lag_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95 lag",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          },
          {
            "expr": "histogram_quantile(0.50, rate(album_assembly_lag_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p50 lag",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 1
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Albums in Progress",
        "type": "stat",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "count(album_items_count_gauge{status=\"pending\"})",
            "refId": "A",
            "legendFormat": "Albums pending",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Album Items Count (by Status)",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "sum(album_items_count_gauge) by (status)",
            "refId": "A",
            "legendFormat": "{{status}}",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Album Aggregation Duration (p95)",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(album_aggregation_duration_ms_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95 aggregation duration",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Album Vision Summary Size",
        "type": "graph",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus",
          "name": "Prometheus"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(album_vision_summary_size_bytes_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95 size",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus",
              "name": "Prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 0
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Active Alerts",
        "type": "alertlist",
        "targets": [],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "name": "Album Alerts",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus",
            "name": "Prometheus"
          },
          "enable": true,
          "iconColor": "red",
          "tags": [
            "album"
          ]
        }
      ]
    },
    "uid": "album-pipeline",
    "id": null
  }
}