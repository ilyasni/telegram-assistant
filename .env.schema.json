{
  "": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "JWT_SECRET",
    "ANON_KEY",
    "SERVICE_KEY",
    "DATABASE_URL",
    "REDIS_URL",
    "QDRANT_URL"
  ],
  "properties": {
    "JWT_SECRET": { "type": "string", "minLength": 16 },
    "ANON_KEY": { "type": "string", "minLength": 20 },
    "SERVICE_KEY": { "type": "string", "minLength": 20 },
    "DATABASE_URL": { "type": "string", "pattern": "^postgresql://" },
    "REDIS_URL": { "type": "string", "pattern": "^redis://" },
    "QDRANT_URL": { "type": "string", "pattern": "^http" }
  },
  "additionalProperties": true
}
